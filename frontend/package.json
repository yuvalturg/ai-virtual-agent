{
  "name": "virtual-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,yaml,yml}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@patternfly/chatbot": "^6.3.2",
    "@patternfly/react-core": "^6.3.1",
    "@patternfly/react-icons": "^6.3.1",
    "@patternfly/react-styles": "^6.3.1",
    "@patternfly/react-tokens": "^6.3.1",
    "@tanstack/react-form": "^1.9.1",
    "@tanstack/react-query": "^5.75.5",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "^1.119.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tanstack/router-plugin": "^1.119.0",
    "@types/node": "^22.15.13",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-dom": "^1.48.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
